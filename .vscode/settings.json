{
  "cSpell.customDictionaries": {
    "project-dictionary": {
      "name": "Workspace",
      "path": "${workspaceRoot}/.vscode/dictionary.txt",
      "description": "Words used in this project",
      "addWords": true,
      "scope": "workspace"
    },
    "custom": true,
    "internal-terms": false
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
}